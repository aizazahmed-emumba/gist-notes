{
    
    "preset": "ts-jest",
    "setupFilesAfterEnv": ["@testing-library/react","@testing-library/jest-dom","<rootDir>/jest.setup.js"],
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
        "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
        "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
}